{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/.github/**",
      "package.json",
      "package-lock.json",
      ".eslintrc.json",
      "next.config.js"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "profstatapi",
          "region": "us-central1"
        }
      }
    ]
  }
}